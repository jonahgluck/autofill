{
    "manifest_version": 2,
  
    "name": "Autofill",
    "description": "Autofill for Supreme & Shopify",
    "version": "1.0",
  
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "Autofill"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["index.js"]
        }
    ],
    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ]
}
